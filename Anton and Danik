#include<iostream>
using namespace std;
int main(){
    int a;
    cin>>a;
    string x;
    cin>>x;

 int count_A=0;
    int count_D=0;
    for(int i=0;i<a;i++){
        if (x[i]=='A')
          count_A+=1;
        else if(x[i]=='D')
         count_D+=1;
    }
    if(count_A>count_D)
     cout<<"Anton";
    else if(count_A<count_D)
    cout<<"Danik";
    else
    cout<<"Friendship";
    return 0;
}
