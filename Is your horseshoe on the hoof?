#include<iostream>
using namespace std;
int main(){
 int arr[4];
for(int i=0;i<4;i++){
cin>>arr[i];
}
int count=0;

    if(arr[1]==arr[0] || arr[1]==arr[2] || arr[1]==arr[3]  )
        count+=1;
    if(arr[2]==arr[0]|| arr[2]==arr[3])
       count+=1;
    if(arr[0]==arr[3])
        count+=1;

cout<<count;


    return 0;
}
